import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        int tens = n / 10;
        int units = n % 10;
        int sum = tens + units;

        System.out.println("Bird said:" + n);
        System.out.println("Alice must go in path-" + sum);
    }
}

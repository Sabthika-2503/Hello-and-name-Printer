import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        if (n > 0) {
            int rev = 0;

            for (; n != 0; n = n / 10) {
                rev = rev * 10 + (n % 10);
            }

            System.out.println(rev);
        } else {
            System.out.println("Invalid Input");
        }
    }
}

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        if (n > 99) {  
            int sum = 0;

              for (int i= n; i > 0; i = i / 10) {
                sum = sum + (i % 10);
            }

            System.out.println("Sum of digit is " + sum);
        } else {
            System.out.println("Invalid Input");
        }
    }
}

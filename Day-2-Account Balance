import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sab = new Scanner(System.in);
        
        int balance = sab.nextInt();
        int choice = sab.nextInt();
        
        switch (choice) {
            case 1:
                int deposit = sab.nextInt();
                System.out.println(balance + deposit);
                break;
            case 2:
                int withdraw = sab.nextInt();
                if(withdraw > balance)
                     System.out.println("Insufficient Balance");
                else
                    System.out.println(balance - withdraw);
                    break;
            default:
                System.out.println("Invalid Input");
        }
    }
}
